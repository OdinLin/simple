{
  "create_api": {
    "name": "api_name",
    "slug": "api_slug",
    "auth": {
      "auth_header_name": "Authorization"
    },
    "definition": {
      "location": "header",
      "key": "x-api-version"
    },
    "version_data": {
      "not_versioned": true,
      "versions": {
        "Default": {
          "name": "Default",
          "use_extended_paths": true
        }
      }
    },
    "proxy": {
      "listen_path": "api_listen",
      "target_url": "api_target",
      "strip_listen_path": true
    },
    "active": true
  }
}